int is_knob_pressed(){
  if (digitalRead(encoder_button) == 0) {
    return 1;
  } else {
    return 0;
  }
}


int is_key1_pressed(){
    if (digitalReadFast(button1) == 0) {       
         return 1;
    }
    else if (digitalReadFast(button2) == 0) {        
         return 2;
    }
    else if (digitalReadFast(button3) == 0) {        
         return 3;
    }
    else if (digitalReadFast(button4) == 0) {        
        return 4;
    }else{
      return 0;
    }
  }
  
  
int is_knob_turned(){}


void UpdateA()
{
  cli();
  _EncoderBSet = digitalReadFast(encoder_B); // read the input pin
  _EncoderTicks = _EncoderBSet ? (_EncoderTicks -1) : (_EncoderTicks+1);
  sei();
}


void PlotMenu(int numerostato){
  myGLCD.setColor(255, 0, 0);
  myGLCD.fillRect(0, 0, 319, 13);
  myGLCD.setColor(255, 0, 255);
  myGLCD.fillRect(0, 226, 319, 239);
  myGLCD.setColor(255, 255, 255);
  myGLCD.setBackColor(255, 0, 0);
  myGLCD.print("* RaRFBio *", CENTER, 1);
  myGLCD.setBackColor(255, 0, 255);
  myGLCD.setColor(255,255,0);
  myGLCD.print("<https://github.com/rafrbio/>", CENTER, 227);
  for (int i = 0; i < NUMEROSTATI; i++){
  int rowposition = 14 + i*heightrow;
  myGLCD.setColor(164, 164, 164);
  myGLCD.setBackColor(164, 164, 164);
  if (i == numerostato) {
  myGLCD.setColor(0, 102, 204);
  myGLCD.setBackColor(0, 102, 204);
  }
  myGLCD.fillRoundRect(0, rowposition, widthmenu, rowposition+heightrow);
  myGLCD.setColor(0, 0, 0);
  myGLCD.drawRoundRect(0, rowposition, widthmenu, rowposition+heightrow);
  myGLCD.setColor(255, 0, 0);
  myGLCD.print(stati[i], 5 , rowposition + heightrow/2);
  }
}


void BackToMenu(){
  stato_corrente = -1;
  myGLCD.clrScr();
  PlotMenu(encoderPos);
  }


void DrawAxis(){
  myGLCD.setColor(255, 255, 255);
  myGLCD.setBackColor(255, 255, 255);
  myGLCD.fillRect(0, 0, widthscreen-1, heightscreen-1);
  myGLCD.setColor(0, 0, 0);
  myGLCD.setBackColor(0, 0, 0);
  myGLCD.drawLine(xcenter, 0, xcenter, 240);
  myGLCD.drawLine(0, ycenter, 319, ycenter); 
  }


void PlotSingleGraph(int new_value,int max_value, int min_value){
  for (int pos=0; pos<widthscreen-1; pos ++){
    sample[pos] = sample [pos+1];
    if(pos+1 != xcenter &&  sample [pos+1] != ycenter ){
    myGLCD.setColor(255, 255, 255);
    myGLCD.drawPixel(pos+1, sample [pos+1]);
    }
   myGLCD.setColor(0, 0, 0);
   myGLCD.drawPixel(pos, sample [pos]);
    }
  int range = abs(max_value-min_value);
  double scaled_value = (1-(new_value-min_value)*1.0/range)*heightscreen;
  sample[widthscreen-1] = int(scaled_value);
  myGLCD.drawPixel(widthscreen-1, sample [widthscreen-1]);
  }


void loop(){

  noInterrupts();

  interrupts();
  
  if(abs(encoderPos - _EncoderTicks ) > 3 ){
  encoderPos = encoderPos < _EncoderTicks ? (encoderPos +1)%NUMEROSTATI : (encoderPos -1)%NUMEROSTATI ;
  encoderPos = encoderPos < 0 ? encoderPos + NUMEROSTATI : encoderPos;
  _EncoderTicks = encoderPos;
  if (stato_corrente < 0){
  PlotMenu(encoderPos);
  }
  }


  if (is_knob_pressed()){
  stato_corrente = encoderPos;

  switch (stato_corrente) {
  case 1: 
  DrawAxis();
  delay(500);
  for(int l=0; l < widthscreen; l++){
     if( is_key1_pressed() > 0 ){
         botton = is_key1_pressed();
     }
 
    if(botton == 1){
    PlotSingleGraph(100*sin(l*1.0/36), 100, -100);
    }else{
     PlotSingleGraph(50, 100, -100);
    }
    delay(100);
  }
  
  delay(10000);
  BackToMenu();
  break;
  
  case 2:
  myGLCD.clrScr();
  myGLCD.setColor(255, 0, 0);
  myGLCD.fillRoundRect(80, 70, 239, 169); // Logo to be put here 
  
  myGLCD.setColor(255, 255, 255);
  myGLCD.setBackColor(255, 0, 0);
  myGLCD.print("Welcome by M&M", CENTER, 93);
  myGLCD.print(":D", CENTER, 139, 90);
  delay(2000);
  BackToMenu();
    break;

  default:
    PlotMenu(encoderPos);
  }
  }
}


